{
  "name": "alexa-metronome-skill",
  "version": "1.0.0",
  "description": "Alexa Metronome Skill",
  "private": true,
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "pretest": "npm run build:prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "build": "webpack --config webpack.config.ts --require ts-node/register",
    "build:dev": "npm run build -- --mode development --watch",
    "build:prod": "npm run lint && npm run build -- --mode production",
    "deploy:prod": "ask deploy --force --profile default --target lambda",
    "convert-assets": "rsvg-convert -w 108 -o assets/icon108.png assets/icon.svg && rsvg-convert -w 512 -o assets/icon512.png assets/icon.svg",
    "create-s3-bucket": "aws s3 ls s3://alexa-metronome-skill > /dev/null 2>&1 || aws s3 mb s3://alexa-metronome-skill --region eu-west-1",
    "deploy:assets": "aws s3 sync ./assets s3://alexa-metronome-skill/ --acl public-read"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/node": "10.14.1",
    "ask-cli": "1.7.4",
    "ask-sdk-core": "2.6.0",
    "ask-sdk-model": "1.16.0",
    "aws-sdk": "2.480.0",
    "class.extend": "0.9.2",
    "i18next": "17.0.4",
    "i18next-sprintf-postprocessor": "0.2.2",
    "jest": "24.8.0",
    "raw-loader": "3.0.0",
    "ts-loader": "6.0.4",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.5.2",
    "underscore": "1.9.1",
    "virtual-alexa": "0.7.2",
    "webpack": "4.35.0",
    "webpack-command": "0.4.2"
  }
}
